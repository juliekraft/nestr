<h1>Create a Listing</h1>

<%= form_for @new_apt do |f| %>
  <%= f.label :price %>
  <%= f.number_field :price %> <br/>

  <%= f.label :bedrooms %>
  <%= f.number_field :bedrooms %> <br/>

  <%= f.label :bathrooms %>
  <%= f.number_field :bathrooms %> <br/>

  <%= f.label :street %>
  <%= f.text_field :street  %> <br />

  <%= f.label :city %>
  <%= f.text_field :city  %> <br />

  <%= f.label :state %>
  <%= f.text_field :state %><br />
  
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode  %><br />

  <%= f.label :region %>
  <%= f.text_field :region  %><br />

  <%= f.label :longitude %>
  <%= f.text_field :longitude  %><br />

  <%= f.label :latitude %>
  <%= f.text_field :latitude %><br />
  
  <%= f.label :photo %>
  <%= f.text_field :photo  %><br />
  
  <%= f.submit :submit %>

<% end %>